---
title: Power Pack
---

<div class="header">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <h1 class="visually-hidden">Power Pack <%= location_name %></h1>
        <a href="/">
        <%= image_tag 'power-pack-logo.svg', class: 'power-pack-logo' %>
        </a>
        <span class="pack-name"><%= location_name %></span>
        <nav class="main-nav">
          <ul>
            <li><a href="about">About</a></li>
            <li><a href="how-it-works">How it works</a></li>
          </ul>
        </nav>
      </div>
    </div>
  </div>
</div>
<div class="strapline">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <h2>Yay! There's already a pack in your area</h2>
      </div>
    </div>
  </div>
</div>

<div id='map'></div>

<div class="signup">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <h2>Join <%= location_name %> Power Pack</h2>
        <%= partial('partials/signup-variable') %>
      </div>
    </div>
  </div>
</div>

<%= partial('partials/footer') %>

<%= partial('partials/mapbox-variable') %>
