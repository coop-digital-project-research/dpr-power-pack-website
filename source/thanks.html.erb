<div class="header">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <h1 class="visually-hidden">Power Pack</h1>
        <a href="/">
          <%= image_tag 'power-pack-logo.svg', class: 'power-pack-logo' %>
        </a>
      </div>
    </div>
  </div>
</div>

<div class="strapline">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <h2>Almost finished...</h2>
      </div>
    </div>
  </div>
</div>

<div class="container transactional">
  <div class="row">
    <div class="col-8">
      <h2>We need to check your email address</h2>
      <p>
        To complete the joining process, please click the link in the email we just sent you. (This may take a few minutes and you might need to check in your Junk folder.)
      </p>
    </div>
  </div>
</div>

<div class="footer">
  <div class="container">
    <div class="row">
      <div class="col-8">
        <p>Any questions? Email <a href="mailto:sophy@powerpack.org.uk">sophy@powerpack.org.uk</a></p>
      </div>
      <div class="col-4">
        <%= image_tag 'coop-logo-white.svg' %>
      </div>
    </div>
  </div>
</div>

