<!doctype html>
<html>
  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title %></title>

    <%= stylesheet_link_tag "site" %>
    <%= stylesheet_link_tag "https://api.tiles.mapbox.com/mapbox-gl-js/v0.33.1/mapbox-gl.css" %>
    <%= javascript_include_tag "site" %>
    <%= javascript_include_tag "https://api.tiles.mapbox.com/mapbox-gl-js/v0.33.1/mapbox-gl.js" %>
    <%= javascript_include_tag "https://code.jquery.com/jquery-3.2.1.min.js" %>
      <%= favicon_tag 'images/favicon.ico' %>
      <script>(function(){var w=window;var ic=w.Intercom;if(typeof ic==="function"){ic('reattach_activator');ic('update',intercomSettings);}else{var d=document;var i=function(){i.c(arguments)};i.q=[];i.c=function(args){i.q.push(args)};w.Intercom=i;function l(){var s=d.createElement('script');s.type='text/javascript';s.async=true;s.src='https://widget.intercom.io/widget/fgxz8mjd';var x=d.getElementsByTagName('script')[0];x.parentNode.insertBefore(s,x);}if(w.attachEvent){w.attachEvent('onload',l);}else{w.addEventListener('load',l,false);}}})()</script>
  </head>

  <body class="<%= page_classes %>">
    <%= yield %>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-94123832-1', 'auto');
      ga('send', 'pageview');

    </script>
  </body>
</html>
